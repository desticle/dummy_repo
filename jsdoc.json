{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["assets/scripts", "code-to-unit-test"],
    "exclude": ["node_modules", "tests", ".github", "assets"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}
